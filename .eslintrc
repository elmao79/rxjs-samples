{
    "extends": "airbnb",
    "rules": {
        "indent": [2, 4],
        "max-len": [1, 150, 2, {ignoreComments: true}],
        "import/newline-after-import": ["error", { "count": 0}],
        "no-console": 0,
        "prefer-const": 0,
        "prefer-destructuring": 0,
        "no-trailing-spaces": 0,
        "comma-dangle": 0
    }
}
